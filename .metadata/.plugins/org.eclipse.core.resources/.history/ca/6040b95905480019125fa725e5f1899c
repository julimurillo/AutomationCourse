import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class Login {

	WebDriver myDriver;
	WebElement txtName;
	WebElement txtPasswd;
	WebElement btnLogin;
	WebElement message;

	public Login(WebDriver driver) {

		this.myDriver = driver;
		txtName.findElement(By.xpath("/html/body/center/div/form/table/tbody/tr[1]/td[2]/input"));
		txtPasswd.findElement(By.xpath("/html/body/center/div/form/table/tbody/tr[2]/td[2]/input"));
		btnLogin.findElement(By.xpath("/html/body/center/div/form/table/tbody/tr[3]/td[2]/input"));
		message.findElement(By.xpath("//*[@id=\"errorMessage\"]"));
	}
	
	public void loginApp(String user, String passwd) {
		 
	}
	
    public void validateLogin() {
		
	}
	
	

}
