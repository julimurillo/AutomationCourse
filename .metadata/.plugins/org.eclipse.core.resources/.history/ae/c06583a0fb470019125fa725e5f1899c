package com.qvision.calculator;

/**
 * Hello world!
 *
 */
public class Calculator 
{
   public int sumar (int a, int b) {
	   
	  return a+b;
	   	   
   }
   
   public int resta (int a, int b) {
	   
		  return a-b;
		   	   
	   }
   
   public int multiplica (int a, int b) {
	   
		  return a*b;
		   	   
	   }
   
   public int divide (int a, int b) {
	  
	   if((a|b) == 0) {
		   System.out.println("error");
	   }
        return a/b;		 
		   	   
	   }
}
